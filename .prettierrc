{
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "cursorOffset": -1,
  "embeddedLanguageFormatting": "off",
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "overrides": [],
  "printWidth": 160,
  "proseWrap": "never",
  "quoteProps": "as-needed",
  "semi": true,
  "singleAttributePerLine": true,
  "requirePragma": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
